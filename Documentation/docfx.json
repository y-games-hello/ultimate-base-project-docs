{
    "metadata": [
      {
        "src": [
          {
            "src": "..",
            "files": [
              "Assets/**/*.cs"
            ],
            "exclude": [
              "Assets/Plugins/**"
            ]
          }
        ],
        "references": [
          "UnityEngine.dll",
          "UnityEditor.dll",
          "Unity.TextMeshPro.dll",
          "UnityEngine.UI.dll"
        ],
        "globalNamespaceId": "Global",
        "filter": "filterConfig.yml",
        "dest": "api",
        "disableGitFeatures": true,
        "template": [
          "default",
          "pdf.default"
        ]
      }
    ],
    "build": {
        "globalMetadata": {
            "_appTitle": "Ultimate Base Project",
            "_appFooter": "Ultimate Base Project",
            "_appLogoPath": "resources/logo.svg",
            "_enableSearch": true,
            "_disableContribution": true,
            "_appFaviconPath": "resources/favicon.ico",
            "pdf": true
        },
        "content": [
            {
                "files": [
                    "toc.yml",
                    "index.md"
                ]
            },
            {
                "src": "api",
                "files": [
                    "*.yml"
                ],
                "dest": "api"
            },
            {
                "src": "manual",
                "files": [
                    "toc.yml",
                    "*.md",
                    "**/*.md"
                ],
                "dest": "manual"
            }
        ],
        "overwrite": [
            {
                "src": "..",
                "files": [
                    "Assets/Scripts/**/*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "resources/**/*",
                    "styles/pdf.css"
                ]
            }
        ],
        "sitemap": {
            "baseUrl": "https://y-games-hello.github.io/ultimate-base-project",
            "changefreq": "weekly",
            "fileOptions": {
                "api/*": {
                    "changefreq": "daily"
                }
            }
        },
        "xref": [
            "https://normanderwan.github.io/UnityXrefMaps/xrefmap.yml"
        ],
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "dest": "_site"
    }
}